{
  "name": "electron-react-app",
  "version": "0.1.0",
  "private": true,
  "description": "React FastAPI Application",
  "author": {
    "name": "LRocha",
    "email": "rochasaitama@hotmail.com"
  },
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "lint": "eslint --ext .ts,.tsx src",
    "preview": "vite preview",
    "electron:dev": "node scripts/electron-dev.js",
    "electron:build": "electron-builder",
    "package:win": "electron-builder --win --x64"
  },
  "build": {
    "appId": "com.yourcompany.electronreactapp",
    "productName": "Electron React App",
    "files": [
      "dist",
      "electron"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1"
  }
}
